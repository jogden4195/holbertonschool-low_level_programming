#include <stdio.h>

/**
 * main - prints all possible combinations of two digits
 *
 * Return: 0
 */
int main(void)
{
	int num1, num2;
	
	for (num1 = '0'; num1 <= '9'; num1++)
	{
		for (num2 = '0'; num2 <= '9'; num2++)
		{
			if(num2 != num1)
			{
			  	putchar(num1);
				putchar(num2);
			}y
			if (num2 == '9' || num1 == '8')
				break;
			putchar(',');
			putchar(' ');
		}
		num2 = num1;
	}
	putchar('\n');
	return (0);
}