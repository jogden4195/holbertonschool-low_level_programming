/**
 * _strcmp - compares two strings
 * @s1 - first string to compare
 * @s2 - second string to compare
 *
 * Return: int
 */

int _strcmp(char *s1, char *s2)
{
	int i = 0;

	while (s1[i] != '\0')
	{
		if (s1[i] == s2[i])
			i++;
		else
		{
			if (s1[i] < s2[i])
				return (-15);
			else
				return (15);
		}
	}
	if (s2[i] != '\0')
		return (-15);
	return (0);
}
